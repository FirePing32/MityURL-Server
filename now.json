{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "routes": [
        {
          "src": "/",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/favicon.ico",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
           "src": "/:id",
           "dest": "/index.js",
           "methods": ["GET"]
          }
      ]
}